    "editorGroupHeader.noTabsBackground": "#eeece2",
    "editorGroupHeader.border": "#d5d4cc",

    "tab.activeBackground": "#eeece2",
    "tab.activeForeground": "#2a2419",
    "tab.activeBorder": "#da7756",
    "tab.inactiveBackground": "#e5e4dc",
    "tab.inactiveForeground": "#5a5952",
    "tab.border": "#d5d4cc",
