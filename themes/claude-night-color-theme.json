    "peekViewResult.fileForeground": "#e8e6de",
    "peekViewResult.lineForeground": "#a8a7a0",
    "peekViewResult.matchHighlightBackground": "#da775650",
    "peekViewResult.selectionBackground": "#da775640",
    "peekViewResult.selectionForeground": "#e8e6de",
    "peekViewTitle.background": "#252422",
    "peekViewTitleDescription.foreground": "#a8a7a0",
    "peekViewTitleLabel.foreground": "#e8e6de",

    "diffEditor.insertedTextBackground": "#a3c78f30",
    "diffEditor.removedTextBackground": "#e07b5c30",
    "diffEditor.insertedLineBackground": "#a3c78f20",
    "diffEditor.removedLineBackground": "#e07b5c20",

    "notifications.background": "#201f1d",
    "notifications.foreground": "#e8e6de",
    "notifications.border": "#2e2d2a",
    "notificationCenterHeader.background": "#201f1d",
    "notificationCenterHeader.foreground": "#e8e6de",
    "notificationLink.foreground": "#da7756",

    "gitDecoration.addedResourceForeground": "#a3c78f",
    "gitDecoration.modifiedResourceForeground": "#7db3d8",
    "gitDecoration.deletedResourceForeground": "#e07b5c",
    "gitDecoration.untrackedResourceForeground": "#a3c78f",
    "gitDecoration.ignoredResourceForeground": "#a8a7a0",
    "gitDecoration.conflictingResourceForeground": "#da7756",
    "gitDecoration.submoduleResourceForeground": "#c49dd4",

    "terminal.background": "#201f1d",
    "terminal.foreground": "#e8e6de",
    "terminal.ansiBlack": "#3a3936",
    "terminal.ansiRed": "#e07b5c",
    "terminal.ansiGreen": "#a3c78f",
    "terminal.ansiYellow": "#da7756",
    "terminal.ansiBlue": "#7db3d8",
    "terminal.ansiMagenta": "#c49dd4",
    "terminal.ansiCyan": "#7dbdb8",
    "terminal.ansiWhite": "#e8e6de",
    "terminal.ansiBrightBlack": "#a8a7a0",
    "terminal.ansiBrightRed": "#f0988a",
    "terminal.ansiBrightGreen": "#b8d8a4",
    "terminal.ansiBrightYellow": "#e8a070",
    "terminal.ansiBrightBlue": "#98c8e8",
    "terminal.ansiBrightMagenta": "#d4b8e4",
    "terminal.ansiBrightCyan": "#98d8d4",
    "terminal.ansiBrightWhite": "#f4f3ee",
    "terminal.selectionBackground": "#da775650",
    "terminalCursor.foreground": "#da7756",

    "debugToolBar.background": "#201f1d",
    "debugToolBar.border": "#2e2d2a",

    "editorError.foreground": "#e07b5c",
    "editorWarning.foreground": "#da7756",
    "editorInfo.foreground": "#7db3d8",
    "editorHint.foreground": "#a3c78f",

    "focusBorder": "#da775680",
    "foreground": "#e8e6de",
    "selection.background": "#da775650",
    "widget.shadow": "#00000030",
    "descriptionForeground": "#a8a7a0",
    "errorForeground": "#e07b5c",
    "icon.foreground": "#a8a7a0",

    "textLink.foreground": "#da7756",
    "textLink.activeForeground": "#e8a070",
    "textBlockQuote.background": "#252422",
    "textBlockQuote.border": "#da7756",
    "textCodeBlock.background": "#252422",
    "textPreformat.foreground": "#da7756",
    "textSeparator.foreground": "#2e2d2a",

    "minimap.background": "#201f1d",
    "minimap.selectionHighlight": "#da775670",
    "minimap.findMatchHighlight": "#da775690",
    "minimapSlider.background": "#a8a7a020",
    "minimapSlider.hoverBackground": "#a8a7a040",
    "minimapSlider.activeBackground": "#a8a7a060",

    "settings.headerForeground": "#e8e6de",
    "settings.modifiedItemIndicator": "#da7756",
    "settings.dropdownBackground": "#252422",
    "settings.dropdownForeground": "#e8e6de",
    "settings.dropdownBorder": "#3a3936",
    "settings.checkboxBackground": "#252422",
    "settings.checkboxForeground": "#e8e6de",
    "settings.checkboxBorder": "#3a3936",
    "settings.textInputBackground": "#252422",
    "settings.textInputForeground": "#e8e6de",
    "settings.textInputBorder": "#3a3936",
    "settings.numberInputBackground": "#252422",
    "settings.numberInputForeground": "#e8e6de",
    "settings.numberInputBorder": "#3a3936"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#a8a7a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a3c78f"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#d4b896"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#d4b896"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#e07b5c"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#e07b5c"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#a8a7a0"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#7db3d8"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#b8c8a8"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c49dd4"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#c49dd4"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#c49dd4"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#e8e6de"
      }
    },
    {
      "name": "Variable Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#c8d8b8"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#b8c8a8"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#e07b5c"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#da7756"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#7db3d8"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css"
      ],
      "settings": {
        "foreground": "#c49dd4"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#7db3d8"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.color.css"
      ],
      "settings": {
        "foreground": "#d4b896"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#da7756"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#7dbdb8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#da7756"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e07b5c"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#a3c78f"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#e07b5c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e8e6de",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#e8e6de",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#7db3d8"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#d4b896"
      }
    },
    {
      "name": "Markdown Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#a8a7a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Lists",
      "scope": [
        "markup.list",
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#da7756"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#e07b5c"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#da7756",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#a8a7a0"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.section",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#b8b7b0"
      }
    }
  ],
  "semanticTokenColors": {
    "type": "#c49dd4",
    "type.declaration": "#c49dd4",
    "class": "#c49dd4",
    "class.declaration": "#c49dd4",
    "interface": "#c49dd4",
    "interface.declaration": "#c49dd4",
    "enum": "#c49dd4",
    "enum.declaration": "#c49dd4",
    "enumMember": "#d4b896",
    "function": "#7db3d8",
    "function.declaration": "#7db3d8",
    "method": "#7db3d8",
    "method.declaration": "#7db3d8",
    "variable": "#e8e6de",
    "variable.declaration": "#e8e6de",
    "variable.readonly": "#d4b896",
    "parameter": "#b8c8a8",
    "property": "#c8d8b8",
    "property.declaration": "#c8d8b8",
    "property.readonly": "#d4b896",
    "keyword": "#e07b5c",
    "comment": "#a8a7a0",
    "string": "#a3c78f",
    "number": "#d4b896",
    "regexp": "#7dbdb8",
    "operator": "#a8a7a0",
    "namespace": "#c49dd4",
    "typeParameter": "#c49dd4",
    "decorator": "#da7756"
  }
}
